<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="600.0" minWidth="600.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainWindowController">
   <children>
      <Button fx:id="buttonAdd" layoutX="205.0" layoutY="65.0" mnemonicParsing="false" onAction="#buttonAddPressed" text="add" />
      <Label layoutX="14.0" layoutY="69.0" text="Add layer with" />
      <TextField fx:id="textFieldNeuronsCount" layoutX="96.0" layoutY="65.0" prefHeight="25.0" prefWidth="52.0" />
      <Label layoutX="152.0" layoutY="69.0" text="neurons" />
      <TableView fx:id="tableViewNetwork" layoutX="14.0" layoutY="100.0" prefHeight="200.0" prefWidth="151.0">
        <columns>
          <TableColumn prefWidth="75.0" text="Layer" />
          <TableColumn prefWidth="75.0" text="Neurons" />
        </columns>
      </TableView>
      <Button fx:id="buttonLoadData" layoutX="18.0" layoutY="31.0" mnemonicParsing="false" onAction="#buttonLoadDataPressed" text="Load data" />
   </children>
</AnchorPane>
